{
  "expo": {
    "name": "Levo",
    "slug": "levo",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#58CC02",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.levo.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#58CC02"
      },
      "package": "com.levo.app",
      "versionCode": 1
    },
      "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "color": "#58CC02"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
